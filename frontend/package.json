{
  "name": "project-generator",
  "productName": "projectGenerator",
  "version": "0.1.0",
  "description": "repo for testing a gui using electron for the project generator",
  "license": "MIT",
  "main": "index.js",
  "repository": "github:openframeworks/projectGenerator",
  "author": {
    "name": "zach lieberman",
    "url": "https://openframeworks.cc"
  },
  "engines": {
    "node": ">=12.20.55"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . $npm_package_productName --icon=static/icon/of.icns --out=dist --ignore='dist' --ignore='readme.md' --prune --all --overwrite --electron-version=24.1.2",
    "build:osx": "echo 'DEPRECATED: build:osx is deprecated. please use build:macos instead' && npm run build:macos",
    "build:macos": "electron-packager . $npm_package_productName --icon=static/icon/of.icns --out=dist --ignore='dist' --ignore='readme.md' --prune --overwrite --electron-version=24.1.2 --platform=darwin --arch=universal --x64ArchFiles=Contents/Resources/app/app/projectGenerator",
    "build:macos-m1": "electron-packager . $npm_package_productName --icon=static/icon/of.icns --out=dist --ignore='dist' --ignore='readme.md' --prune --overwrite --electron-version=24.1.2 --platform=darwin --arch=arm64",
    "build:linux32": "electron-packager . $npm_package_productName --icon=static/icon/of.icns --out=dist --ignore='dist' --ignore='readme.md' --prune --overwrite --electron-version=24.1.2 --platform=linux --arch=ia32",
    "build:linux64": "electron-packager . $npm_package_productName --icon=static/icon/of.icns --out=dist --ignore='dist' --ignore='readme.md' --prune --overwrite --electron-version=24.1.2 --platform=linux --arch=x64",
    "build:vs": "electron-packager . --icon=static/icon/of.ico --out=dist --ignore='dist' --ignore='readme.md' --prune --overwrite --electron-version=24.1.2 --platform=win32 --arch=ia32"
  },
  "files": [
    "index.html",
    "app/*",
    "static/*"
  ],
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "moniker": "^0.1.2",
    "n-readlines": "^1.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.60",
    "electron": "^24.1.2",
    "electron-packager": "^17.1.1"
  }
}
